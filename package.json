{
  "name": "sub-box",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:up": "bun with-env drizzle-kit up",
    "db:generate": "bun with-env drizzle-kit generate",
    "db:push": "bun with-env drizzle-kit push",
    "db:push:local": "bun with-env bun with-local-db drizzle-kit push --config drizzle.local.config.ts",
    "db:migrate": "bun with-env drizzle-kit migrate",
    "db:migrate:local": "bun with-env bun with-local-db drizzle-kit migrate --config drizzle.local.config.ts",
    "db:check": "bun with-env drizzle-kit check",
    "db:studio": "bun with-env drizzle-kit studio",
    "db:studio:local": "bun with-env bun with-local-db drizzle-kit studio --config drizzle.local.config.ts",
    "d1:migrate:local": "cross-env DATABASE_NAME=prod-sub-box-db wrangler d1 migrations apply $DATABASE_NAME --local",
    "d1:migrate:remote": "cross-env DATABASE_NAME=prod-sub-box-db wrangler d1 migrations apply $DATABASE_NAME --remote",
    "dev": "bun with-env next dev",
    "build": "bun with-env next build",
    "lint": "bun with-env next lint",
    "tc": "bun with-env tsc",
    "dev:worker": "wrangler dev",
    "build:worker": "bun with-env opennextjs-cloudflare build",
    "preview": "bun build:worker && wrangler dev",
    "deploy": "bun build:worker && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "bindings": "cf-bindings-proxy",
    "with-env": "dotenv -e .dev.vars --",
    "with-local-db": "cross-env LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit)"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "latest",
    "@libsql/client": "^0.15.4",
    "@opennextjs/cloudflare": "^1.0.0-beta.3",
    "@radix-ui/react-accordion": "^1.2.7",
    "@radix-ui/react-alert-dialog": "^1.1.10",
    "@radix-ui/react-aspect-ratio": "^1.1.4",
    "@radix-ui/react-avatar": "^1.1.6",
    "@radix-ui/react-checkbox": "^1.2.2",
    "@radix-ui/react-collapsible": "^1.1.7",
    "@radix-ui/react-context-menu": "^2.2.11",
    "@radix-ui/react-dialog": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.11",
    "@radix-ui/react-hover-card": "^1.1.10",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-menubar": "^1.1.11",
    "@radix-ui/react-navigation-menu": "^1.2.9",
    "@radix-ui/react-popover": "^1.1.10",
    "@radix-ui/react-progress": "^1.1.4",
    "@radix-ui/react-radio-group": "^1.3.3",
    "@radix-ui/react-scroll-area": "^1.2.5",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slider": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.8",
    "@radix-ui/react-toast": "^1.2.10",
    "@radix-ui/react-toggle": "^1.1.6",
    "@radix-ui/react-toggle-group": "^1.1.7",
    "@radix-ui/react-tooltip": "^1.2.3",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/ms": "^2.1.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "crypto": "latest",
    "date-fns": "4.1.0",
    "dotenv-cli": "^8.0.0",
    "drizzle-orm": "^0.41.0",
    "embla-carousel-react": "8.5.1",
    "fs": "0.0.2",
    "input-otp": "1.4.1",
    "jose": "^5.10.0",
    "lucide-react": "^0.471.2",
    "ms": "^2.1.3",
    "next": "14.2.23",
    "next-themes": "^0.4.6",
    "openapi-fetch": "^0.13.5",
    "path": "latest",
    "react": "^18.3.1",
    "react-day-picker": "8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "latest",
    "react-resizable-panels": "^2.1.7",
    "recharts": "2.15.0",
    "server-only": "latest",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.9",
    "yaml": "^2.7.1",
    "zod": "latest"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250419.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@opennextjs/aws": "^3.5.8",
    "@types/node": "^22.14.1",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.30.6",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.1.4",
    "openapi-typescript": "^7.6.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "wrangler": "^3.114.6"
  },
  "packageManager": "bun@1.1.18"
}
